{
    "compilerOptions": {
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "module": "none",
        "moduleResolution": "node",
        "noEmitOnError": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "removeComments": true,
        "sourceMap": true,
        "inlineSourceMap": false,
        "target": "es5",
        "lib": [
            "dom",
            "es5"
        ]
    }
}
